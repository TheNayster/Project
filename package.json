{
  "name": "nucampsite",
  "version": "1.0.0",
  "description": "",
  "main": "Hobbies.html",
  "scripts": {
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin Pets.html -d dist --htmlmin -o dist/Pets.html && usemin Eddy.html -d dist --htmlmin -o dist/Eddy.html && usemin Hobbies.html -d dist --htmlmin -o dist/Hobbies.html",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.13.1",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "again": "0.0.1",
    "bootstrap": "4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.0",
    "popper.js": "^1.12.9"
  }
}
